{
  "name": "@open-scaffold/client",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "Open Scaffold Client - Abstract client contract and HTTP implementation",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./internal": {
      "import": {
        "types": "./dist/internal.d.mts",
        "default": "./dist/internal.mjs"
      },
      "require": {
        "types": "./dist/internal.d.cts",
        "default": "./dist/internal.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/open-scaffold/open-scaffold",
    "directory": "packages/client"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@open-scaffold/core": "*",
    "effect": "latest"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@open-scaffold/server": "*",
    "@open-scaffold/testing": "*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "jsdom": "^27.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
