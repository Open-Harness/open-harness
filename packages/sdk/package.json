{
	"name": "@dao/sdk",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"bmad": "claude --mcp-config .mcp.json",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"lint:summary": "biome check --reporter summary .",
		"capture": "bun scripts/e2e-capture.ts",
		"harvest": "bun scripts/harvest.ts",
		"dev": "bun --hot src/server/index.ts",
		"build": "bun build ./src/server/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
		"start": "NODE_ENV=production bun src/server/index.ts",
		"test": "bun test tests/**/*.test.ts",
		"test:unit": "bun test tests/unit/container.test.ts",
		"test:integration": "bun test tests/integration/live-sdk.test.ts",
		"example": "bun src/examples/autonomous-harness.ts"
	},
	"dependencies": {
		"@anthropic-ai/claude-agent-sdk": "^0.1.76",
		"@needle-di/core": "^1.1.0",
		"react": "^19",
		"react-dom": "^19",
		"reflect-metadata": "^0.2.2",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@types/bun": "latest",
		"@types/react": "^19",
		"@types/react-dom": "^19"
	},
	"module": "src/index.ts",
	"peerDependencies": {
		"typescript": "^5"
	}
}
